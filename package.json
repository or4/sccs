{
  "name": "node-typescript-boilerplate",
  "author": "Maik Ellerbrock <opensource@frapsoft.com> (https://github.com/ellerbrock)",
  "homepage": "https://github.com/ellerbrock/node-typescript-boilerplate",
  "version": "0.0.1",
  "description": "Node.js TypeScript Boilerplate",
  "main": "build/index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn run env:dev && nodemon --exec ts-node -- example",
    "prebuild": "rimraf build",
    "build": "tsc",
    "lint": "tslint src/*.ts && tslint example/*.ts && tslint test/*.ts",
    "commit": "git add --all && git status",
    "upd": "ncu -ua && yarn",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production"
  },
  "keywords": [
    "boilerplate",
    "skeleton",
    "starter",
    "ts",
    "typescirpt-node",
    "typescript",
    "typescript-boilerplate",
    "typescript-skeleton",
    "typescript-starter"
  ],
  "devDependencies": {
    "@types/node": "^10.0.3",
    "@types/pg": "^7.4.10",
    "@types/ramda": "^0.25.36",
    "babel-eslint": "^8.2.6",
    "cross-env": "^5.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-alloy": "^1.4.2",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-lodash": "^2.5.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-typescript": "^0.12.0",
    "nodemon": "^1.11.0",
    "npm-check-updates": "^2.8.7",
    "pg": "7.4.3",
    "ramda": "^0.25.0",
    "rimraf": "^2.5.4",
    "ts-node": "^7.0.0",
    "tsdoc": "^0.0.4",
    "tslint": "^5.0.0",
    "typescript": "^2.9.2-rc",
    "typescript-eslint-parser": "^15.0.0",
    "yarn": "^1.0.1"
  }
}
